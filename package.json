    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.22.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.20.20",
    "typescript": "^4.1.3"
  },
  "optionalDependencies": {
    "isomorphic-fetch": "^3.0.0"

{
  "name": "geotab-rx",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/types/index.d.ts",
  "author": "Marvin Countryman <me@maar.vin>",
  "version": "0.2.0-alpha",
  "license": "MIT",
  "repository": "https://github.com/mcountryman/geotab.git",
  "description": "Unofficial Typescript Geotab SDK.",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "node ./tools/build.js",
    "build:doc": "typedoc",
    "test": "jest",
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "generate": "yarn generate:clean && yarn generate:dotnet && yarn generate:pretty",
    "generate:clean": "rimraf ./src/models",
    "generate:dotnet": "dotnet run --project tools/sicilian",
    "generate:pretty": "eslint --write ./src/models"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/geotab-rx.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/geotab-rx.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "rxjs": "^6.6.3",
    "uuid-random": "^1.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^17.1.0",
    "@size-limit/preset-small-lib": "^4.9.2",
    "@types/isomorphic-fetch": "^0.0.35",
    "husky": "^5.0.9",
    "rimraf": "^3.0.2",
    "rollup": "^2.38.5",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "size-limit": "^4.9.2",
    "tslib": "^2.1.0",
    "typedoc": "^0.20.20",
    "typescript": "^4.1.5"
  },
  "optionalDependencies": {
    "isomorphic-fetch": "^3.0.0"
  }
}
